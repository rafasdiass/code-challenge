{
  "name": "quave-code-challenge",
  "private": true,
  "scripts": {
    "start": "meteor run --exclude-archs web.browser.legacy,web.cordova",
    "quave-eslint": "eslint .",
    "quave-prettier": "prettier --write \"**/*.js\"",
    "quave-check": "npm run quave-eslint && npm run quave-prettier",
    "build:css": "tailwindcss -i ./client/input.css -o ./client/output.css",
    "prettier": "prettier --write \"**/*.js\""
  },
  "dependencies": {
    "@babel/runtime": "^7.24.7",
    "meteor-node-stubs": "^1.2.9",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "winston": "^3.8.2"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@quave/eslint-config-quave": "^3.0.0-beta.0",
    "@types/meteor": "^2.9.8",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-define-config": "^1.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lefthook": "^1.6.18",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4"
  }
}
